{
  "name": "relay-modern-typescript-transformer",
  "version": "0.0.2",
  "description": "A transformer for Typescript that transforms GraphQL queries to require functions.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "prepare": "tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^8.0.47",
    "@types/react-relay": "^1.3.1",
    "babel-plugin-relay": "^1.4.1",
    "babel-types": "^6.26.0",
    "graphql": "^0.11.7",
    "jest": "^21.2.1",
    "react-relay": "^1.4.1",
    "ts-jest": "^21.1.3",
    "tslib": "^1.8.0",
    "typescript": "^2.5.3"
  },
  "peerDependencies": {
    "graphql": "*",
    "typescript": "*"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(ts|tsx)$"
  }
}
